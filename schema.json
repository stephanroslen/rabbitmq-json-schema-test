{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "something",
  "type": "object",
  "required": [
    "some_string",
    "some_number"
  ],
  "properties": {
    "some_string": {
      "type": "string",
      "description": "Personal information",
      "properties": {},
      "required": []
    },
    "some_number": {
      "type": "number",
      "description": "List of hobbies",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the hobby"
          },
          "yearsExperience": {
            "type": "number",
            "description": "Years of experience"
          }
        }
      }
    }
  }
}